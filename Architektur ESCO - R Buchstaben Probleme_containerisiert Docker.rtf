\cf4 \strokec4 # 1. Konfiguration & DB-Verbindung (l\'e4dt Daten aus deiner .env)\cf3 \strokec3 \
load_dotenv()\
DB_URL = os.getenv(\cf15 \strokec15 "DATABASE_URL"\cf3 \strokec3 , \cf15 \strokec15 "postgresql://postgres:pass@localhost:5432/job_mining"\cf3 \strokec3 )\
\
\cf12 \strokec12 def\cf3 \strokec3  \cf17 \strokec17 get_data\cf3 \strokec3 ():\
    conn = psycopg2.connect(DB_URL)\
    \cf4 \strokec4 # Wir holen uns die ESCO-Matches und das Jahr f\'fcr die Trendanalyse\cf3 \strokec3 \
    query = \cf15 \strokec15 """\
        SELECT esco_match, EXTRACT(YEAR FROM posting_date) as year, count(*) as count\
        FROM job_postings, unnest(esco_skills) as esco_match\
        WHERE esco_match IS NOT NULL AND esco_match != 'Kein ESCO Match'\
        GROUP BY esco_match, year\
        ORDER BY count DESC\
