    selected_industries = st.sidebar.multiselect(\cf15 \strokec15 "Branchen"\cf3 \strokec3 , options=df[\cf15 \strokec15 'industry'\cf3 \strokec3 ].unique(), default=df[\cf15 \strokec15 'industry'\cf3 \strokec3 ].unique())\
\
    \cf4 \strokec4 # Daten filtern\cf3 \strokec3 \
    mask = (df[\cf15 \strokec15 'year'\cf3 \strokec3 ].between(*year_range)) & (df[\cf15 \strokec15 'industry'\cf3 \strokec3 ].isin(selected_industries))\
    filtered_df = df[mask]\
