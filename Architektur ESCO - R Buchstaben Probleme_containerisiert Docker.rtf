\
\cf12 \strokec12 try\cf3 \strokec3 :\
    df = get_data()\
\
    \cf4 \strokec4 # Sidebar f\'fcr Filter\cf3 \strokec3 \
    st.sidebar.header(\cf15 \strokec15 "Filter"\cf3 \strokec3 )\
    selected_year = st.sidebar.multiselect(\cf15 \strokec15 "Jahre ausw\'e4hlen"\cf3 \strokec3 , \
                                            options=\cf14 \strokec14 sorted\cf3 \strokec3 (df[\cf15 \strokec15 'year'\cf3 \strokec3 ].unique()),\
                                            default=\cf14 \strokec14 sorted\cf3 \strokec3 (df[\cf15 \strokec15 'year'\cf3 \strokec3 ].unique()))\
\
    filtered_df = df[df[\cf15 \strokec15 'year'\cf3 \strokec3 ].isin(selected_year)]\
\
