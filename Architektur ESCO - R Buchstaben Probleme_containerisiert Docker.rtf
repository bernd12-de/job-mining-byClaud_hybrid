        fig = px.bar(top_skills, x=\cf15 \strokec15 'count'\cf3 \strokec3 , y=\cf15 \strokec15 'esco_match'\cf3 \strokec3 , orientation=\cf15 \strokec15 'h'\cf3 \strokec3 , color=\cf15 \strokec15 'count'\cf3 \strokec3 )\
