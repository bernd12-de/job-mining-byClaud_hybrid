        fig_line = px.line(trend_data, x=\cf15 \strokec15 'year'\cf3 \strokec3 , y=\cf15 \strokec15 'n'\cf3 \strokec3 , color=\cf15 \strokec15 'esco_match'\cf3 \strokec3 , markers=\cf13 \strokec13 True\cf3 \strokec3 )\
        st.plotly_chart(fig_line, use_container_width=\cf13 \strokec13 True\cf3 \strokec3 )\
