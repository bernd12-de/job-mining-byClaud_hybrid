\cf12 \strokec12 def\cf4 \strokec4  \cf13 \strokec13 analyze_skill_trend\cf4 \strokec4 (df_skills):\
    \cf15 \strokec15 """\
    Erwartet ein DataFrame mit den Spalten: 'year', 'skill_name', 'count', 'total_ads'\
    """\cf4 \strokec4 \
    \cf5 \strokec5 # 1. Normalisierung: Skill-Vorkommen pro 1000 Anzeigen\cf4 \strokec4 \
    df_skills[\cf15 \strokec15 'normalized'\cf4 \strokec4 ] = (df_skills[\cf15 \strokec15 'count'\cf4 \strokec4 ] / df_skills[\cf15 \strokec15 'total_ads'\cf4 \strokec4 ]) * \cf16 \strokec16 1000\cf4 \strokec4 \
    \
    \cf5 \strokec5 # 2. Berechnung der prozentualen Ver\'e4nderung zum Vorjahr\cf4 \strokec4 \
    df_skills[\cf15 \strokec15 'growth_rate'\cf4 \strokec4 ] = df_skills[\cf15 \strokec15 'normalized'\cf4 \strokec4 ].pct_change() * \cf16 \strokec16 100\cf4 \strokec4 \
    \
    \cf5 \strokec5 # 3. Trend-Bewertung (Wissenschaftlicher Standard)\cf4 \strokec4 \
    avg_growth = df_skills[\cf15 \strokec15 'growth_rate'\cf4 \strokec4 ].mean()\
    \
    status = \cf15 \strokec15 "Stabil"\cf4 \strokec4 \
    \cf12 \strokec12 if\cf4 \strokec4  avg_growth > \cf16 \strokec16 15\cf4 \strokec4 : status = \cf15 \strokec15 "Emerging (Starkes Wachstum)"\cf4 \strokec4 \
    \cf12 \strokec12 elif\cf4 \strokec4  avg_growth < -\cf16 \strokec16 15\cf4 \strokec4 : status = \cf15 \strokec15 "Declining (R\'fcckl\'e4ufig)"\cf4 \strokec4 \
    \
    \cf12 \strokec12 return\cf4 \strokec4  \{\
        \cf15 \strokec15 "data"\cf4 \strokec4 : df_skills.to_dict(orient=\cf15 \strokec15 'records'\cf4 \strokec4 ),\
        \cf15 \strokec15 "avg_annual_growth"\cf4 \strokec4 : \cf14 \strokec14 round\cf4 \strokec4 (avg_growth, \cf16 \strokec16 2\cf4 \strokec4 ),\
        \cf15 \strokec15 "trend_label"\cf4 \strokec4 : status\
    \}\
