\cf12 \strokec12 class\cf13 \strokec13  DocumentExtractor:\cf4 \strokec4 \
    \cf15 \strokec15 """\
    Robuster Extractor f\'fcr PDF, DOCX und TXT, priorisiert pdftotext (Poppler) f\'fcr PDF.\
    """\cf4 \strokec4 \
    \
    \cf12 \strokec12 def\cf4 \strokec4  \cf13 \strokec13 __init__\cf4 \strokec4 (self):\
        \cf5 \strokec5 # Pr\'fcft, ob Poppler/pdftotext im System verf\'fcgbar ist (KRITISCH)\cf4 \strokec4 \
        self._has_pdftotext = \cf14 \strokec14 bool\cf4 \strokec4 (subprocess.getstatusoutput(\cf15 \strokec15 "pdftotext -v"\cf4 \strokec4 )[\cf16 \strokec16 0\cf4 \strokec4 ] == \cf16 \strokec16 0\cf4 \strokec4 )\
\
    \cf12 \strokec12 def\cf4 \strokec4  \cf13 \strokec13 _read_txt\cf4 \strokec4 (self, path: Path) -> str:\
        \cf12 \strokec12 return\cf4 \strokec4  path.read_text(encoding=\cf15 \strokec15 'utf-8'\cf4 \strokec4 , errors=\cf15 \strokec15 'ignore'\cf4 \strokec4 )\
\
    \cf12 \strokec12 def\cf4 \strokec4  \cf13 \strokec13 _read_docx\cf4 \strokec4 (self, path: Path) -> str:\
        \cf15 \strokec15 """Robuster DOCX-Parser (nutzt python-docx)"""\cf4 \strokec4 \
        \cf12 \strokec12 try\cf4 \strokec4 :\
            doc = Document(\cf14 \strokec14 str\cf4 \strokec4 (path))\
            parts = [p.text \cf12 \strokec12 for\cf4 \strokec4  p \cf12 \strokec12 in\cf4 \strokec4  doc.paragraphs \cf12 \strokec12 if\cf4 \strokec4  p.text.strip()]\
            \
            \cf5 \strokec5 # Text aus Tabellen hinzuf\'fcgen\cf4 \strokec4 \
            \cf12 \strokec12 for\cf4 \strokec4  table \cf12 \strokec12 in\cf4 \strokec4  doc.tables:\
                \cf12 \strokec12 for\cf4 \strokec4  row \cf12 \strokec12 in\cf4 \strokec4  table.rows:\
                    row_text = \cf15 \strokec15 " "\cf4 \strokec4 .join(cell.text.strip() \cf12 \strokec12 for\cf4 \strokec4  cell \cf12 \strokec12 in\cf4 \strokec4  row.cells \cf12 \strokec12 if\cf4 \strokec4  cell.text)\
                    \cf12 \strokec12 if\cf4 \strokec4  row_text: parts.append(row_text)\
\
            \cf12 \strokec12 return\cf4 \strokec4  \cf15 \strokec15 '\\n'\cf4 \strokec4 .join(parts)\
        \cf12 \strokec12 except\cf4 \strokec4  Exception:\
            \cf5 \strokec5 # Fallback auf den XML-Zip-Ansatz aus alter parser.py (weniger robust, aber vorhanden)\cf4 \strokec4 \
            \cf12 \strokec12 try\cf4 \strokec4 :\
                \cf12 \strokec12 with\cf4 \strokec4  zipfile.ZipFile(path) \cf12 \strokec12 as\cf4 \strokec4  z:\
                    xml_content = z.read(\cf15 \strokec15 "word/document.xml"\cf4 \strokec4 ).decode(\cf15 \strokec15 "utf-8"\cf4 \strokec4 , errors=\cf15 \strokec15 "ignore"\cf4 \strokec4 )\
                    xml_content = re.sub(\cf15 \strokec15 r'</w:p>'\cf4 \strokec4 , \cf15 \strokec15 '\\n'\cf4 \strokec4 , xml_content)\
                    xml_content = re.sub(\cf15 \strokec15 r'<(.|\\n)*?>'\cf4 \strokec4 , \cf15 \strokec15 ''\cf4 \strokec4 , xml_content)\
                    \cf12 \strokec12 return\cf4 \strokec4  re.sub(\cf15 \strokec15 r'\\s+'\cf4 \strokec4 , \cf15 \strokec15 ' '\cf4 \strokec4 , xml_content).strip()\
            \cf12 \strokec12 except\cf4 \strokec4  Exception:\
                \cf12 \strokec12 return\cf4 \strokec4  \cf15 \strokec15 ""\cf4 \strokec4 \
\
\
    \cf12 \strokec12 def\cf4 \strokec4  \cf13 \strokec13 _read_pdf_text\cf4 \strokec4 (self, path: Path) -> str:\
        \cf15 \strokec15 """Kaskade: pdftotext (Poppler) -> pdfminer.six (Python Native)"""\cf4 \strokec4 \
        \
        \cf5 \strokec5 # 1. pdftotext (Poppler - sehr stabil)\cf4 \strokec4 \
        \cf12 \strokec12 if\cf4 \strokec4  self._has_pdftotext:\
            \cf12 \strokec12 try\cf4 \strokec4 :\
                out = subprocess.run(\
                    [\cf15 \strokec15 "pdftotext"\cf4 \strokec4 , \cf15 \strokec15 "-layout"\cf4 \strokec4 , \cf15 \strokec15 "-q"\cf4 \strokec4 , \cf14 \strokec14 str\cf4 \strokec4 (path), \cf15 \strokec15 "-"\cf4 \strokec4 ],\
                    check=\cf16 \strokec16 True\cf4 \strokec4 , capture_output=\cf16 \strokec16 True\cf4 \strokec4 , timeout=\cf16 \strokec16 10\cf4 \strokec4 \
                )\
                text = out.stdout.decode(\cf15 \strokec15 "utf-8"\cf4 \strokec4 , \cf15 \strokec15 "ignore"\cf4 \strokec4 ).strip()\
                \cf12 \strokec12 if\cf4 \strokec4  \cf14 \strokec14 len\cf4 \strokec4 (text.strip()) > \cf16 \strokec16 50\cf4 \strokec4 : \cf12 \strokec12 return\cf4 \strokec4  text\
            \cf12 \strokec12 except\cf4 \strokec4  Exception:\
                \cf12 \strokec12 pass\cf4 \strokec4  \cf5 \strokec5 # Fallback auf Python-Native\cf4 \strokec4 \
\
        \cf5 \strokec5 # 2. PDFMiner (Python-native)\cf4 \strokec4 \
        \cf12 \strokec12 try\cf4 \strokec4 :\
            \cf12 \strokec12 from\cf4 \strokec4  pdfminer.high_level \cf12 \strokec12 import\cf4 \strokec4  extract_text\
            text = extract_text(\cf14 \strokec14 str\cf4 \strokec4 (path))\
            \cf12 \strokec12 if\cf4 \strokec4  \cf14 \strokec14 len\cf4 \strokec4 (text.strip()) > \cf16 \strokec16 50\cf4 \strokec4 : \cf12 \strokec12 return\cf4 \strokec4  text\
        \cf12 \strokec12 except\cf4 \strokec4  Exception:\
            \cf12 \strokec12 pass\cf4 \strokec4 \
\
        \cf12 \strokec12 return\cf4 \strokec4  \cf15 \strokec15 ""\cf4 \strokec4  \cf5 \strokec5 # Leerer String bei Fehlschlag\cf4 \strokec4 \
\
    \cf12 \strokec12 def\cf4 \strokec4  \cf13 \strokec13 extract_text\cf4 \strokec4 (self, file_path: Path) -> Tuple[str, str]:\
        \cf15 \strokec15 """Extrahiert den Rohtext und gibt den Dateityp zur\'fcck."""\cf4 \strokec4 \
        p_str = \cf14 \strokec14 str\cf4 \strokec4 (file_path).strip().strip(\cf15 \strokec15 '"'\cf4 \strokec4 ).strip(\cf15 \strokec15 "'"\cf4 \strokec4 )\
        p = Path(p_str)\
        ext = p.suffix.lower()\
\
        \cf12 \strokec12 if\cf4 \strokec4  ext == \cf15 \strokec15 '.pdf'\cf4 \strokec4 :\
            \cf12 \strokec12 return\cf4 \strokec4  self._read_pdf_text(p), \cf15 \strokec15 'pdf'\cf4 \strokec4 \
        \cf12 \strokec12 elif\cf4 \strokec4  ext \cf12 \strokec12 in\cf4 \strokec4  (\cf15 \strokec15 '.docx'\cf4 \strokec4 , \cf15 \strokec15 '.doc'\cf4 \strokec4 ):\
            \cf12 \strokec12 return\cf4 \strokec4  self._read_docx(p), \cf15 \strokec15 'docx'\cf4 \strokec4 \
        \cf12 \strokec12 elif\cf4 \strokec4  ext \cf12 \strokec12 in\cf4 \strokec4  (\cf15 \strokec15 '.txt'\cf4 \strokec4 , \cf15 \strokec15 '.md'\cf4 \strokec4 ):\
            \cf12 \strokec12 return\cf4 \strokec4  self._read_txt(p), \cf15 \strokec15 'txt'\cf4 \strokec4 \
        \
        \cf12 \strokec12 return\cf4 \strokec4  \cf15 \strokec15 ""\cf4 \strokec4 , \cf15 \strokec15 ""\cf4 \strokec4 \
\
    \cf12 \strokec12 def\cf4 \strokec4  \cf13 \strokec13 clean_text\cf4 \strokec4 (self, text: \cf14 \strokec14 str\cf4 \strokec4 ) -> str:\
        \cf15 \strokec15 """Implementiert die notwendige aggressive Textreinigung (aus alter main.py)."""\cf4 \strokec4 \
        \cf12 \strokec12 if\cf4 \strokec4  \cf12 \strokec12 not\cf4 \strokec4  text: \cf12 \strokec12 return\cf4 \strokec4  \cf15 \strokec15 ""\cf4 \strokec4 \
        text = re.sub(\cf15 \strokec15 r'http[s]?://\\S+'\cf4 \strokec4 , \cf15 \strokec15 ''\cf4 \strokec4 , text)\
        text = re.sub(\cf15 \strokec15 r'\\S+@\\S+'\cf4 \strokec4 , \cf15 \strokec15 ''\cf4 \strokec4 , text)\
        text = re.sub(\cf15 \strokec15 r'\\s+'\cf4 \strokec4 , \cf15 \strokec15 ' '\cf4 \strokec4 , text).strip()\
        \cf12 \strokec12 return\cf4 \strokec4  text\
