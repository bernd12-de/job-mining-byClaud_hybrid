        text = text.replace(\cf15 \strokec15 '\'e4'\cf4 \strokec4 , \cf15 \strokec15 'ae'\cf4 \strokec4 ).replace(\cf15 \strokec15 '\'f6'\cf4 \strokec4 , \cf15 \strokec15 'oe'\cf4 \strokec4 ).replace(\cf15 \strokec15 '\'fc'\cf4 \strokec4 , \cf15 \strokec15 'ue'\cf4 \strokec4 ).replace(\cf15 \strokec15 '\'df'\cf4 \strokec4 , \cf15 \strokec15 'ss'\cf4 \strokec4 )\
