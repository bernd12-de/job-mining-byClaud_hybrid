    \cf12 \strokec12 public\cf4 \strokec4  List<SkillDeltaDTO> \cf13 \strokec13 calculateGlobalDelta\cf4 \strokec4 (\cf12 \strokec12 int\cf4 \strokec4  year1, \cf12 \strokec12 int\cf4 \strokec4  year2) \{\
        \cf5 \strokec5 // 1. Holen der aggregierten Daten \'fcber die Repository-Query\cf4 \strokec4 \
        List<Object[]> rawData = jobRepository.compareSkillsBetweenYears(year1, year2);\
        \
        \cf5 \strokec5 // 2. Gesamtanzahl der Anzeigen pro Jahr f\'fcr die Normalisierung holen\cf4 \strokec4 \
        \cf12 \strokec12 long\cf4 \strokec4  totalAdsYear1 = jobRepository.countByPostingYear(year1);\
        \cf12 \strokec12 long\cf4 \strokec4  totalAdsYear2 = jobRepository.countByPostingYear(year2);\
\
        \cf12 \strokec12 return\cf4 \strokec4  rawData.stream().map(row -> \{\
            String name = (String) row[\cf16 \strokec16 0\cf4 \strokec4 ];\
            \cf12 \strokec12 long\cf4 \strokec4  count1 = ((Number) row[\cf16 \strokec16 1\cf4 \strokec4 ]).longValue();\
            \cf12 \strokec12 long\cf4 \strokec4  count2 = ((Number) row[\cf16 \strokec16 2\cf4 \strokec4 ]).longValue();\
\
            \cf5 \strokec5 // Berechnung der relativen H\'e4ufigkeit in Prozent\cf4 \strokec4 \
            \cf12 \strokec12 double\cf4 \strokec4  relFreq1 = (totalAdsYear1 > \cf16 \strokec16 0\cf4 \strokec4 ) ? (count1 * \cf16 \strokec16 100.0\cf4 \strokec4  / totalAdsYear1) : \cf16 \strokec16 0\cf4 \strokec4 ;\
            \cf12 \strokec12 double\cf4 \strokec4  relFreq2 = (totalAdsYear2 > \cf16 \strokec16 0\cf4 \strokec4 ) ? (count2 * \cf16 \strokec16 100.0\cf4 \strokec4  / totalAdsYear2) : \cf16 \strokec16 0\cf4 \strokec4 ;\
            \cf12 \strokec12 double\cf4 \strokec4  delta = relFreq2 - relFreq1;\
\
            String status = \cf15 \strokec15 "Stable"\cf4 \strokec4 ;\
            \cf12 \strokec12 if\cf4 \strokec4  (delta > \cf16 \strokec16 5.0\cf4 \strokec4 ) status = \cf15 \strokec15 "Emerging"\cf4 \strokec4 ; \cf5 \strokec5 // \'dcber 5% Zuwachs\cf4 \strokec4 \
            \cf12 \strokec12 else\cf4 \strokec4  \cf12 \strokec12 if\cf4 \strokec4  (delta < -\cf16 \strokec16 5.0\cf4 \strokec4 ) status = \cf15 \strokec15 "Declining"\cf4 \strokec4 ; \cf5 \strokec5 // \'dcber 5% Verlust\cf4 \strokec4 \
\
            \cf12 \strokec12 return\cf4 \strokec4  \cf12 \strokec12 new\cf4 \strokec4  SkillDeltaDTO(name, relFreq1, relFreq2, delta, status);\
        \})\
