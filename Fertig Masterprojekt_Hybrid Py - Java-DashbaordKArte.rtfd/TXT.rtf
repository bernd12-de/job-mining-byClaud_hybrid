    \cf12 \strokec12 def\cf4 \strokec4  \cf13 \strokec13 categorize\cf4 \strokec4 (self, job: JobPosting) -> JobPosting:\
        \cf5 \strokec5 # Extrahiere Titel und Rohtext (normalisiert)\cf4 \strokec4 \
        text = \cf15 \strokec15 f"\cf2 \strokec2 \{job.title \cf12 \strokec12 or\cf2 \strokec2  \cf15 \strokec15 ''\cf2 \strokec2 \}\cf15 \strokec15  \cf2 \strokec2 \{job.raw_text \cf12 \strokec12 or\cf2 \strokec2  \cf15 \strokec15 ''\cf2 \strokec2 \}\cf15 \strokec15 "\cf4 \strokec4 .lower()\
\
        \cf5 \strokec5 # Priorisierte Kategorien\cf4 \strokec4 \
