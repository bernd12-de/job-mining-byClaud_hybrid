    \cf12 \strokec12 def\cf4 \strokec4  \cf13 \strokec13 generate_csv_report\cf4 \strokec4 (self) -> io.StringIO:\
        \cf15 \strokec15 """Erstellt einen CSV-Report aus allen in der DB gespeicherten Jobs."""\cf4 \strokec4 \
        \
        jobs: List[JobPosting] = self._repository.fetch_all_job_postings()\
        \
        \cf12 \strokec12 if\cf4 \strokec4  \cf12 \strokec12 not\cf4 \strokec4  jobs:\
            logger.warning(\cf15 \strokec15 "Keine Jobs in der Datenbank gefunden, generiere leeren Report."\cf4 \strokec4 )\
            \cf12 \strokec12 return\cf4 \strokec4  io.StringIO()\
            \
        output = io.StringIO()\
        fieldnames = [\
            \cf15 \strokec15 'Job_ID'\cf4 \strokec4 , \cf15 \strokec15 'Titel'\cf4 \strokec4 , \cf15 \strokec15 'Firma'\cf4 \strokec4 , \cf15 \strokec15 'Standort'\cf4 \strokec4 , \cf15 \strokec15 'Jahr'\cf4 \strokec4 , \cf15 \strokec15 'Job_Dom\'e4ne'\cf4 \strokec4 , \cf15 \strokec15 'Remote'\cf4 \strokec4 , \
            \cf15 \strokec15 'Gehalt_Min_EUR'\cf4 \strokec4 , \cf15 \strokec15 'Gehalt_Max_EUR'\cf4 \strokec4 , \cf15 \strokec15 'Erfahrung_Min_Jahre'\cf4 \strokec4 , \
