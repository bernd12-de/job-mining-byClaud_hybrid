\cf5 \strokec5 # Importiere die Muster aus der Infrastruktur-Schicht\cf4 \strokec4 \
\pard\pardeftab720\partightenfactor0
\cf12 \strokec12 from\cf4 \strokec4  infrastructure.dates_extractor \cf12 \strokec12 import\cf4 \strokec4  extract_posted_date \
\cf12 \strokec12 from\cf4 \strokec4  infrastructure.metadata_patterns \cf12 \strokec12 import\cf4 \strokec4  (\
    extract_organization, extract_location, extract_title, \
    extract_section_items, extract_salary, extract_experience, \
    extract_education, extract_languages, extract_year_fallback,\
    check_remote\
)\
\
\cf12 \strokec12 class\cf13 \strokec13  MetadataService:\cf4 \strokec4 \
    \cf15 \strokec15 """\
    Kapselt die Extraktionslogik zur Anreicherung der JobPosting-Entit\'e4t\
    mit allen Metadaten (Basis, Stufe 3) und Klassifizierungen.\
    """\cf4 \strokec4 \
    \
    \cf12 \strokec12 def\cf4 \strokec4  \cf13 \strokec13 __init__\cf4 \strokec4 (self):\
        \cf5 \strokec5 # Der Rollen-Klassifizierer ist eine Anwendungskomponente\cf4 \strokec4 \
        self._role_categorizer = RoleCategorizer() \
\
    \cf12 \strokec12 def\cf4 \strokec4  \cf13 \strokec13 enrich\cf4 \strokec4 (self, job: JobPosting) -> JobPosting:\
        \cf15 \strokec15 """\
        Extrahiert ALLE Metadaten und f\'fcgt sie der JobPosting-Entit\'e4t hinzu.\
        """\cf4 \strokec4 \
        raw_text = job.raw_text\
        \
        \cf5 \strokec5 # 1. BASIS-METADATEN (Konsolidierte Logik)\cf4 \strokec4 \
        job.title = extract_title(raw_text, job.source_path)\
        job.company = extract_organization(raw_text)\
        job.location = extract_location(raw_text)\
        job.is_remote = check_remote(raw_text)\
\
        \cf5 \strokec5 # 2. DATUMSEXTRAKTION (KRITISCH f\'fcr Zeitreihen)\cf4 \strokec4 \
        \cf5 \strokec5 # Nutzt die robuste Logik aus dates_extractor\cf4 \strokec4 \
        dt_obj, _ = extract_posted_date(raw_text)\
        \
        \cf12 \strokec12 if\cf4 \strokec4  dt_obj:\
            job.date_parsed = dt_obj.date()\
            job.year = dt_obj.year\
        \cf12 \strokec12 else\cf4 \strokec4 :\
            \cf5 \strokec5 # Fallback-Jahr, wenn kein Datum gefunden wurde\cf4 \strokec4 \
            job.year = extract_year_fallback(raw_text, job.source_path)\
\
        \cf5 \strokec5 # 3. AUFGABEN/ANFORDERUNGEN (Sektionen-Parser-Logik)\cf4 \strokec4 \
        tasks, requirements = extract_section_items(raw_text)\
        job.tasks_raw.extend(tasks)\
        job.requirements_raw.extend(requirements)\
        \
        \cf5 \strokec5 # 4. STUFE 3 METADATEN (Detail-Analyse)\cf4 \strokec4 \
        \
        \cf5 \strokec5 # Gehalt\cf4 \strokec4 \
        job.salary_min, job.salary_max = extract_salary(raw_text)\
        \
        \cf5 \strokec5 # Erfahrung\cf4 \strokec4 \
        job.experience_years_min, job.experience_years_max = extract_experience(raw_text)\
\
        \cf5 \strokec5 # Bildung/Sprachen\cf4 \strokec4 \
        job.education_level = extract_education(raw_text)\
        job.languages = extract_languages(raw_text)\
        \
        \cf5 \strokec5 # 5. ROLLEN-KLASSIFIZIERUNG\cf4 \strokec4 \
        job = self._role_categorizer.categorize(job)\
\
        \cf12 \strokec12 return\cf4 \strokec4  job\
