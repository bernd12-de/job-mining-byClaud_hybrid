    List<Integer> years = List.of(\cf16 \strokec16 2021\cf4 \strokec4 , \cf16 \strokec16 2022\cf4 \strokec4 , \cf16 \strokec16 2023\cf4 \strokec4 , \cf16 \strokec16 2024\cf4 \strokec4 );\
    \
    \cf5 \strokec5 // 1. Finde die Top 5 Standorte nach Gesamtanzahl Nennungen f\'fcr diesen Skill\cf4 \strokec4 \
    List<String> topLocations = jobRepository.findTop5LocationsBySkill(skillName);\
    \
    Map<String, List<Double>> result = \cf12 \strokec12 new\cf4 \strokec4  LinkedHashMap<>();\
    \
    \cf5 \strokec5 // 2. Daten f\'fcr Top 5 sammeln\cf4 \strokec4 \
    \cf12 \strokec12 for\cf4 \strokec4  (String loc : topLocations) \{\
        result.put(loc, calculateAnnualFreq(skillName, loc, years));\
    \}\
    \
    \cf5 \strokec5 // 3. Daten f\'fcr "Andere Standorte" aggregieren\cf4 \strokec4 \
    List<Double> othersFreq = \cf12 \strokec12 new\cf4 \strokec4  ArrayList<>();\
    \cf12 \strokec12 for\cf4 \strokec4  (Integer year : years) \{\
        \cf12 \strokec12 long\cf4 \strokec4  countOthers = jobRepository.countSkillInOtherLocations(skillName, topLocations, year);\
        \cf12 \strokec12 long\cf4 \strokec4  totalOthers = jobRepository.countAdsInOtherLocations(topLocations, year);\
        othersFreq.add(totalOthers > \cf16 \strokec16 0\cf4 \strokec4  ? (countOthers * \cf16 \strokec16 100.0\cf4 \strokec4  / totalOthers) : \cf16 \strokec16 0.0\cf4 \strokec4 );\
    \}\
    result.put(\cf15 \strokec15 "Andere Standorte (\'d8)"\cf4 \strokec4 , othersFreq);\
    \
    \cf12 \strokec12 return\cf4 \strokec4  result;\
\}\
