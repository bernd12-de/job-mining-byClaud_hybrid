        \
        \cf5 \strokec5 # 4. STUFE 3 METADATEN (Detail-Analyse)\cf4 \strokec4 \
        \
        \cf5 \strokec5 # Gehalt\cf4 \strokec4 \
        salary_min, salary_max = extract_salary(job.raw_text)\
        job.salary_min = salary_min\
        job.salary_max = salary_max\
\
        \cf5 \strokec5 # Erfahrung\cf4 \strokec4 \
        exp_min, exp_max = extract_experience(job.raw_text)\
        job.experience_years_min = exp_min\
        job.experience_years_max = exp_max\
\
        \cf5 \strokec5 # Bildung/Sprachen\cf4 \strokec4 \
        job.education_level = extract_education(job.raw_text)\
        job.languages = extract_languages(job.raw_text)\
        \
        \cf5 \strokec5 # (Job-Domain-Klassifizierung folgt sp\'e4ter im RoleService)\cf4 \strokec4 \
\
        \cf12 \strokec12 return\cf4 \strokec4  job\
\
