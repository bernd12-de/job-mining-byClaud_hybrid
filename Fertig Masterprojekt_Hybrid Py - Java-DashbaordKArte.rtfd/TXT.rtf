    \cf12 \strokec12 def\cf4 \strokec4  \cf13 \strokec13 enrich\cf4 \strokec4 (self, job: JobPosting) -> JobPosting:\
        \
        \cf5 \strokec5 # 1. BASISEXTRAKTIONEN (Konsolidierte Logik)\cf4 \strokec4 \
        job.title = extract_title(job.raw_text, job.source_path)\
        job.company = extract_organization(job.raw_text)\
