            \cf5 \strokec5 // Output von Python lesen\cf4 \strokec4 \
            BufferedReader reader = \cf12 \strokec12 new\cf4 \strokec4  BufferedReader(\cf12 \strokec12 new\cf4 \strokec4  InputStreamReader(process.getInputStream()));\
            StringBuilder output = \cf12 \strokec12 new\cf4 \strokec4  StringBuilder();\
            String line;\
            \cf12 \strokec12 while\cf4 \strokec4  ((line = reader.readLine()) != \cf12 \strokec12 null\cf4 \strokec4 ) \{\
                output.append(line);\
            \}\
\
            \cf12 \strokec12 int\cf4 \strokec4  exitCode = process.waitFor();\
            \cf12 \strokec12 if\cf4 \strokec4  (exitCode == \cf16 \strokec16 0\cf4 \strokec4 ) \{\
                \cf5 \strokec5 // Den JSON-String von Python in ein Java-Objekt (DTO) umwandeln\cf4 \strokec4 \
                \cf12 \strokec12 return\cf4 \strokec4  objectMapper.readValue(output.toString(), PythonResultDTO.class);\
            \} \cf12 \strokec12 else\cf4 \strokec4  \{\
                \cf12 \strokec12 throw\cf4 \strokec4  \cf12 \strokec12 new\cf4 \strokec4  RuntimeException(\cf15 \strokec15 "Python-Skript Fehler. Exit Code: "\cf4 \strokec4  + exitCode);\
            \}\
\
        \} \cf12 \strokec12 catch\cf4 \strokec4  (Exception e) \{\
            e.printStackTrace();\
            \cf12 \strokec12 return\cf4 \strokec4  \cf12 \strokec12 null\cf4 \strokec4 ;\
