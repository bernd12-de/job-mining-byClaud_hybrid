                \cf15 \strokec15 'Job_ID'\cf4 \strokec4 : job.job_id,\
                \cf15 \strokec15 'Titel'\cf4 \strokec4 : job.title \cf12 \strokec12 or\cf4 \strokec4  \cf15 \strokec15 'N/A'\cf4 \strokec4 ,\
                \cf15 \strokec15 'Firma'\cf4 \strokec4 : job.company \cf12 \strokec12 or\cf4 \strokec4  \cf15 \strokec15 'N/A'\cf4 \strokec4 ,\
                \cf15 \strokec15 'Standort'\cf4 \strokec4 : job.location \cf12 \strokec12 or\cf4 \strokec4  \cf15 \strokec15 'N/A'\cf4 \strokec4 ,\
                \cf15 \strokec15 'Jahr'\cf4 \strokec4 : job.year \cf12 \strokec12 or\cf4 \strokec4  \cf15 \strokec15 ''\cf4 \strokec4 ,\
                \cf15 \strokec15 'Job_Dom\'e4ne'\cf4 \strokec4 : job.job_domain \cf12 \strokec12 or\cf4 \strokec4  \cf15 \strokec15 'N/A'\cf4 \strokec4 ,\
                \cf15 \strokec15 'Remote'\cf4 \strokec4 : \cf15 \strokec15 'Ja'\cf4 \strokec4  \cf12 \strokec12 if\cf4 \strokec4  job.is_remote \cf12 \strokec12 else\cf4 \strokec4  \cf15 \strokec15 'Nein'\cf4 \strokec4 ,\
                \cf15 \strokec15 'Gehalt_Min_EUR'\cf4 \strokec4 : job.salary_min \cf12 \strokec12 or\cf4 \strokec4  \cf15 \strokec15 ''\cf4 \strokec4 ,\
                \cf15 \strokec15 'Gehalt_Max_EUR'\cf4 \strokec4 : job.salary_max \cf12 \strokec12 or\cf4 \strokec4  \cf15 \strokec15 ''\cf4 \strokec4 ,\
                \cf15 \strokec15 'Erfahrung_Min_Jahre'\cf4 \strokec4 : job.experience_years_min \cf12 \strokec12 or\cf4 \strokec4  \cf15 \strokec15 ''\cf4 \strokec4 ,\
                \cf15 \strokec15 'Erfahrung_Max_Jahre'\cf4 \strokec4 : job.experience_years_max \cf12 \strokec12 or\cf4 \strokec4  \cf15 \strokec15 ''\cf4 \strokec4 ,\
                \cf15 \strokec15 'Bildungsgrad'\cf4 \strokec4 : job.education_level \cf12 \strokec12 or\cf4 \strokec4  \cf15 \strokec15 'N/A'\cf4 \strokec4 ,\
                \cf15 \strokec15 'Anzahl_Skills'\cf4 \strokec4 : \cf14 \strokec14 len\cf4 \strokec4 (job.competences),\
                \cf15 \strokec15 'Skills_ESCO_Liste'\cf4 \strokec4 : skill_list,\
            \})\
            \
